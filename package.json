{
  "name": "macaca-puppeteer",
  "version": "2.0.0",
  "description": "macaca puppeteer driver",
  "keywords": [
    "puppeteer",
    "macaca"
  ],
  "main": "./lib/macaca-puppeteer",
  "repository": {
    "type": "git",
    "url": "git://github.com/macacajs/macaca-puppeteer.git"
  },
  "dependencies": {
    "driver-base": "^0.1.0",
    "macaca-utils": "~1.0.0",
    "puppeteer": "^2.0.0",
    "selenium-atoms": "~1.0.1",
    "webdriver-dfn-error-code": "^1.0.3",
    "xlogger": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.2.2",
    "git-contributor": "1",
    "husky": "^4.0.10",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "mocha": "*",
    "nyc": "^13.1.0",
    "startserver": "^1.2.3"
  },
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text mocha",
    "lint": "eslint --fix lib test",
    "clean": "rm -rf ./docs",
    "doc": "npm run clean && jsdoc -c ./jsdoc.json",
    "ci": "npm run lint && npm run test",
    "contributor": "git-contributor"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "homepage": "https://github.com/macacajs/macaca-puppeteer",
  "license": "MIT"
